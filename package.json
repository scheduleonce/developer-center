{
  "name": "@oncehub/developer-center",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "npm run openapi:watch & npm start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "convert-legacy-docs": "tsx scripts/convert-legacy-docs.ts",
    "openapi:validate": "redocly lint openapi/index.yaml",
    "openapi:build": "node scripts/build-openapi.js",
    "openapi:build-yaml": "node scripts/build-openapi.js --yaml",
    "openapi:build-json": "node scripts/build-openapi.js --json",
    "openapi:build-variants": "node scripts/build-product-variants.js",
    "openapi:preview": "redocly preview-docs openapi/index.yaml",
    "openapi:watch": "node scripts/build-product-variants.js --watch",
    "openapi:serve": "redocly preview-docs static/oncehub-api.yaml",
    "openapi:bundle": "redocly bundle openapi/index.yaml --output static/oncehub-api.yaml",
    "prebuild": "npm run openapi:build && npm run openapi:build-variants"
