{
  "name": "@oncehub/developer-center",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "dev": "concurrently --prefix none --kill-others --kill-others-on-fail \"npm run openapi:watch\" \"npm start\"",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "test": "concurrently --prefix none \"npm:prettier:check\" \"npm:openapi:validate\"",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "openapi:validate": "redocly lint openapi/index.yaml",
    "openapi:build": "node scripts/build-openapi.js",
    "openapi:watch": "node scripts/build-openapi.js --watch",
    "prebuild": "npm run openapi:build",
    "prepare": "husky"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "3.1.1",
    "@scalar/api-reference": "1.43.8",
    "@scalar/docusaurus": "0.7.30",
    "@signalwire/docusaurus-plugin-llms-txt": "2.0.0-alpha.7",
    "@signalwire/docusaurus-theme-llms-txt": "1.0.0-alpha.9",
    "clsx": "2.1.1",
    "js-yaml": "4.1.1",
    "prism-react-renderer": "2.4.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "@redocly/cli": "2.14.5",
    "@types/node": "25.0.9",
    "concurrently": "9.2.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.0",
    "typescript": "5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,yaml,yml}": "prettier --write"
  }
}
